<?xml version="1.0" encoding="utf-8"?>


  <RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >  
        
	     
<LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
     <LinearLayout
   
   			 android:layout_width="fill_parent"
    		 android:layout_height="50dp"
    		android:background="@drawable/selector_current"
   			 android:orientation="horizontal"
   			 
    		android:padding="2dp" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
         android:background="@drawable/selector_current"
        android:clickable="true"
      
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >


            
            <ImageButton
                android:id="@+id/bplanet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                
               android:background="@drawable/planet"
               />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_current"
        android:clickable="true"

        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

           

            <ImageButton
                android:id="@+id/starbutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                
               android:background="@drawable/fstar"
               />
        </LinearLayout>
    </LinearLayout>
     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:background="@drawable/tab_pressed"
        android:clickable="false"
       
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

           

           <ImageButton
                android:id="@+id/bviewsolarsystem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                
               android:background="@drawable/solar"
               />
        </LinearLayout>
    </LinearLayout>
     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_current"
        android:clickable="true"
     
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

           

            <ImageButton
                android:id="@+id/bconst"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                
               android:background="@drawable/cons"
               />
        </LinearLayout>
    </LinearLayout>



</LinearLayout>

        <RadioGroup
        android:id="@+id/radio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"  >
       
 
     <RadioButton
         android:id="@+id/click2D"
         android:layout_width="wrap_content"
         android:checked="true"
         android:layout_height="wrap_content"
         android:text="@string/check2D" 
         />

     <RadioButton
         android:id="@+id/click3D"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         
         android:text="@string/check3D" />

 
    </RadioGroup>

        <Spinner
            android:id="@+id/spinnersystem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    <WebView
        android:id="@+id/web3D"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout></RelativeLayout>